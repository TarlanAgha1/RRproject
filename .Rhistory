test_accuracy <- sum(test_pred == label_test) / length(label_test)
cat(sprintf("Depth %d: Training score: %f, Testing score: %f\n", depth, train_accuracy, test_accuracy))
return(c(train = train_accuracy, test = test_accuracy))
}
